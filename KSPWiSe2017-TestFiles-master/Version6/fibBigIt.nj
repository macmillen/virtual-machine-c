void main() {
    local Integer x;
    local Integer a;
    local Integer b;
    local Integer i;
    local Integer aa;
    local Integer bb;
    x = readInteger();
    if (x <= 0) {
        writeInteger(0);
    } else if(x == 1) {
        writeInteger(1);
    } else {
        a = 0;
        b = 1;        
        i = 2;
        while(i <= x) {
            aa = b;            
            bb = a + b;
            a = aa;
            b = bb;
            i = i + 1;
        }
        writeInteger(b);
    }
    writeCharacter('\n');
}